<h1>Welcome, Create a New Partnership!</h1>

<%= form_for @vendorsweet do |f| %>
  <%= f.label 'Choose a vendor 💁‍♂️' %>
  <%= f.collection_select :vendor_id, @vendors, :id, :name %><br>
  <%= f.label 'Choose a sweet 🍬' %>
  <%= f.collection_select :sweet_id, @sweets, :id, :name %><br><br>
  <%= f.submit %>
  <% end %>
